  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colorfy-software/emittify/issues"
  },
  "homepage": "https://github.com/colorfy-software/emittify#readme",
  "devDependencies": {
    "@types/react": "^18.0.17",
    "release-it": "^15.4.0",
    "solid-js": "^1.4.8",
    "typescript": "^4.7.4"
  },
  "release-it": {
